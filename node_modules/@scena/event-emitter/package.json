{
    "name": "@scena/event-emitter",
    "version": "1.0.5",
    "description": "Implement EventEmitter on object or component",
    "main": "./dist/event-emitter.cjs.js",
    "module": "./dist/event-emitter.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "scripts": {
        "start": "rollup -c -w",
        "build": "rollup -c && npm run declaration && print-sizes ./dist",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
        "doc": "rm -rf ./doc && jsdoc -c jsdoc.json",
        "prerelease": "npm run build && npm run doc && prerelease --dirs=dist,doc",
        "release": "npm run build && npm run doc && release --dirs=dist,doc",
        "release:init": "npm run build && npm run doc && release -i --dirs=dist,doc",
        "deploy": "gh-pages -d ./demo --dest=./ --add --remote origin",
        "deploy:init": "gh-pages -d ./demo --dest=./ --remote origin",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daybrush/gesture.git"
    },
    "author": "Daybrush",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daybrush/gesture/issues"
    },
    "homepage": "https://github.com/daybrush/gesture",
    "dependencies": {
        "@daybrush/utils": "^1.1.1"
    },
    "devDependencies": {
        "@daybrush/builder": "^0.1.2",
        "@daybrush/release": "^0.2.4",
        "@daybrush/jsdoc": "^0.3.7",
        "@types/jest": "^24.0.13",
        "@typescript-eslint/eslint-plugin": "^3.9.1",
        "@typescript-eslint/parser": "^3.9.1",
        "coveralls": "^3.0.3",
        "daybrush-jsdoc-template": "^1.6.0",
        "eslint": "^7.7.0",
        "gh-pages": "^3.1.0",
        "print-coveralls": "^1.2.2",
        "print-sizes": "0.0.4",
        "rollup": "^2.32.0",
        "typescript": "^3.4.5"
    }
}
